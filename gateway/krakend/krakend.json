                    "clientMaxRate": 5,
                    "strategy": "ip"
                },
                "github.com/devopsfaith/krakend-jose/validator": {
                    "alg": "RS256",
                    "audience": ["https://api.hubbedin.com"],
                    "jwk-url": "https://hubbed-in.au.auth0.com/.well-known/jwks.json"
                }
            },
            "backend": [
                {
                    "host": [ "gateway:8000" ],
