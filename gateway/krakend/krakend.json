{
    "version": 2,
    "name": "HubbedIn API",
    "port": 8080,
    "cache_ttl": "300s",
    "timeout": "3s",
    "plugin": {
        "pattern":".so",
        "folder": "./"
    },
    "extra_config": {
        "github_com/devopsfaith/krakend-gologging": {
            "level":  "DEBUG",
            "prefix": "[KRAKEND]",
            "syslog": false,
            "stdout": true
        },
        "github_com/devopsfaith/krakend-httpsecure": {
            "allowed_hosts": [
            ],
            "sts_include_subdomains": true,
            "frame_deny": true,
            "custom_frame_options_value": "ALLOW-FROM hubbedin.com",
            "hpkp_public_key": "pin-sha256=\"base64==\"; max-age=expireTime [; includeSubDomains][; report-uri=\"reportURI\"]",
            "content_type_nosniff": true,
            "browser_xss_filter": true,
            "content_security_policy": "default-src 'self';"
        },
        "github_com/devopsfaith/krakend-cors": {
            "allow_origins": [
                "https://hubbedin.dev:3000",
                "https://candidate.hubbedin.com"
            ],
            "expose_headers": [
                "Content-Length",
                "Content-Type"
            ],
            "max_age": "12h",
            "allow_methods": [
                "GET",
                "HEAD",
                "POST",
                "PUT",
                "DELETE",
                "OPTIONS"
            ],
            "allow_headers": [
                "Authorization", 
                "Content-Type"
            ],
            "allow_credentials": false
        }
    },
    "endpoints": [
        {
            "endpoint": "/v1/candidates",
            "method": "POST",
            "output_encoding": "json",
            "headers_to_pass": [
                "Authorization"
            ],
            "extra_config": {
                "github.com/devopsfaith/krakend-ratelimit/juju/router": {
                    "maxRate": 100,
                    "clientMaxRate": 5,
                    "strategy": "ip"
                }
            },
            "backend": [
                {
                    "host": [ "gateway:8000" ],
                    "method": "POST",
                    "url_pattern": "/v1/candidates"
                }
            ]
        },
        {
